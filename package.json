{
  "name": "software-update-manager",
  "version": "1.0.0",
  "description": "A CurseForge-like application for managing software updates",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev-server": "cross-env BROWSER=none webpack serve --config webpack.config.js",
    "start-electron": "electron .",
    "start-backend": "nodemon server/index.js",
    "dev": "concurrently -n web,electron,server \"npm run dev-server\" \"wait-on tcp:3000 && npm run start-electron\" \"npm run start-backend\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "file-drop-element": "^1.0.1",
    "mongoose": "^8.1.3",
    "notistack": "^3.0.1",
    "puppeteer": "^24.1.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.9",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "electron": "^28.2.3",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^9.3.0",
    "style-loader": "^3.3.4",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
